{
    "$": {
        "publish": {
        	"archive": "site.zip"
        }, 
        "variables": {},  
        "pageDefaults": {
            "waitFor": [
            	"head > title:not(:empty)", 
            	"main h2", 
            	"footer > a"
            ], 
            "replace": {
            	"head > meta[resource='ex:#']": "", 
            	"head > meta[is='e-data'][resource=':root|h1 > a']": "", 
            	"script[src$='jsonata.min.js']": "", 
            	"/[\\n\\r\\t\\s]+[\\n\\r\\t]+[\\n\\r\\t\\s]+/": "\n"
            }, 
            "sinkData": {
            	"a[href^='#']": "${\"@href\": $substring($targetValueOf.`@href`, 1) }", 
            	"style[data-stylesheet-id]": {"stylesheetId": null}, 
            	"head > base": {"@href": "/"}
            }
        }, 
        "trigger": { "altKey": false, "ctrlKey": false, "metaKey": true, "shiftKey": true, "key": "P" }
    }, 
	"pages": {
		"": "", 
		"page1": {
			"url": "index.html#page1", 
			"replace": {
				"script[src$='jsonata.min.js']": null
			}
		}, 
		"page2": "index.html#page2"
	}, 
	"files": [
		"element/widget.html"
	]
}